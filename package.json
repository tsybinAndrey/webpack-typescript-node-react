{
  "name": "webpack-typescript-node-react",
  "version": "0.0.0",
  "description": "Simple example of how to set up development environment with docker webpack node.js and react",
  "main": "index.ts",
  "scripts": {
    "start": "",
    "server:prod": "node build/server/server.bundle.js",
    "server:build": "webpack --config webpack/webpack.server.js --color -p --progress --hide-modules --display-optimization-bailout",
    "server:dev": "webpack --config webpack/webpack.server.js --watch & nodemon --watch build/server build/server/server.bundle.js",
    "front:build": "webpack --config webpack/webpack.front.prod.js --color -p --progress --hide-modules --display-optimization-bailout",
    "tsc:check": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "typescript",
    "node.js",
    "webpack",
    "docker"
  ],
  "author": "Andrew Tcybin",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/express": "^4.17.1",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/webpack": "^4.39.5",
    "@types/webpack-dev-middleware": "^2.0.3",
    "babel-loader": "^8.0.6",
    "core-js": "^3.3.3",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.4",
    "regenerator-runtime": "^0.13.3",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  }
}
